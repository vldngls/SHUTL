{
  "name": "shutl",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "dev": "nodemon backend/server.js",
    "install:frontend": "cd frontend && npm install && npm update",
    "install:backend": "cd backend && npm install && npm update",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build": "npm install && npm update && npm run install:frontend && npm run install:backend && npm run build:frontend && npm run build:backend"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.6.0",
    "mongoose": "^8.6.0",
    "prop-types": "^15.8.1",
    "qrcode.react": "^4.1.0",
    "react-icons": "^5.3.0",
    "react-leaflet": "^4.2.1",
    "shutl": "file:",
    "socket.io-client": "^4.8.0",
    "supports-color": "^9.4.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  }
